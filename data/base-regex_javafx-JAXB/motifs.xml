<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:tableMotifsEntityJAXB xmlns:ns2="levy.daniel.application">
    <listeMotifs>
        <motif>
            <nom>1 chiffre</nom>
            <motifJava>\d</motifJava>
            <signification>1 chiffre</signification>
            <alias>[0-9]</alias>
            <motifJavaScript>/ \d /</motifJavaScript>
        </motif>
        <motif>
            <nom>commence par 1 à 3 chiffres</nom>
            <motifJava>^\d{1,3}?</motifJava>
            <signification>commence par 1 à 3 chiffres reluctant</signification>
            <alias>^\d[0-9]{1,3}?</alias>
            <motifJavaScript>/ ^\\d{1,3}? /</motifJavaScript>
        </motif>
        <motif>
            <nom>commence par 1 à 4 chiffres</nom>
            <motifJava>^\d{1,4}?</motifJava>
            <signification>commence par 1 à 4 chiffres reluctant</signification>
            <alias>^\d[0-9]{1,4}?</alias>
            <motifJavaScript>/ ^\d{1,4}? /</motifJavaScript>
        </motif>
    </listeMotifs>
</ns2:tableMotifsEntityJAXB>
